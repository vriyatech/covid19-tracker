	},
	"devDependencies": {
		"browserify": "^16.5.0",
		"catty": "0.1.0",
		"csv-spectrum": "^1.0.0",
		"deep-eql": ">=0.1.3",
		"mocha": "^5.2.0",
		"shell-quote": "^1.6.1",
		"underscore": "^1.9.0"
	},
	"bin": {
